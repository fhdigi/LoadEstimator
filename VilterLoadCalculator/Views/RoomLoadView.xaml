<Window x:Class="VilterLoadCalculator.Views.RoomLoadView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VilterLoadCalculator"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="321" VerticalAlignment="Top" Width="518" Margin="0,0,0,-0.2">
            <TabItem Header="Room Load">
                <Grid Background="#FFE5E5E5">
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Grid HorizontalAlignment="Left" Height="292" Margin="0,0,-0.2,0" VerticalAlignment="Top" Width="513"/>
                    <Label x:Name="label" Content="Fluid" HorizontalAlignment="Left" Margin="62,11,0,36.6" Width="40" RenderTransformOrigin="0.696,0.578" Grid.RowSpan="2"/>
                    <ComboBox x:Name="comboBox" Grid.ColumnSpan="3" HorizontalAlignment="Left" Margin="102,11,0,0" VerticalAlignment="Top" Width="103" Height="26" Grid.RowSpan="2"/>
                    <Label x:Name="label1" Content="Flow" HorizontalAlignment="Left" Margin="64,10.2,0,0" Grid.Row="1" VerticalAlignment="Top"/>
                    <TextBox x:Name="textBox" Grid.ColumnSpan="3" HorizontalAlignment="Left" Height="23" Margin="102,14.2,0,0" Grid.Row="1" Grid.RowSpan="2" TextWrapping="Wrap" VerticalAlignment="Top" Width="103"/>
                    <Label x:Name="label2" Content="Temperature to maintain" Grid.Column="2" HorizontalAlignment="Left" Margin="65.2,11,0,0" VerticalAlignment="Top" Grid.ColumnSpan="3" Grid.RowSpan="2" Width="140"/>
                    <Label x:Name="label3" Content="Temperature Enviorment" Grid.Column="2" HorizontalAlignment="Left" Margin="65.2,10.2,0,0" Grid.Row="1" VerticalAlignment="Top" Grid.ColumnSpan="3" Width="140"/>
                    <TextBox x:Name="textBox1" Grid.Column="3" HorizontalAlignment="Left" Height="23" Margin="101.8,15,-0.2,0" Grid.RowSpan="2" TextWrapping="Wrap" VerticalAlignment="Top" Width="104" Grid.ColumnSpan="2"/>
                    <TextBox x:Name="textBox2" Grid.Column="3" HorizontalAlignment="Left" Height="23" Margin="101.8,14.2,-0.2,0" Grid.Row="1" Grid.RowSpan="2" TextWrapping="Wrap" VerticalAlignment="Top" Width="104" Grid.ColumnSpan="2"/>
                    <Button x:Name="button" Content="Calculate" HorizontalAlignment="Left" Height="36" Margin="102,36.4,0,0" Grid.Row="2" VerticalAlignment="Top" Width="103" Grid.ColumnSpan="3" Grid.RowSpan="2"/>
                    <Label x:Name="label4" Content="Room Load" HorizontalAlignment="Left" Margin="31.6,13.8,0,0" Grid.Row="6" VerticalAlignment="Top" Grid.ColumnSpan="2" Grid.Column="1" Grid.RowSpan="2"/>
                    <TextBox x:Name="textBox3" Grid.ColumnSpan="3" Grid.Column="2" HorizontalAlignment="Left" Height="23" Margin="0.2,12.8,0,0" Grid.Row="6" TextWrapping="Wrap" VerticalAlignment="Top" Width="205"/>
                    <Label x:Name="label5" Content="Tons" Grid.Column="4" HorizontalAlignment="Left" Margin="0.4,17.8,0,0" Grid.Row="6" VerticalAlignment="Top" Grid.RowSpan="2"/>
                </Grid>
            </TabItem>
            <TabItem Header="Process Load">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
